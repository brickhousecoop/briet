{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "bh-briet-market#build": {
      "env": [
        "SANITY_TOKEN",
        "STRIPE_SECRET_KEY",
        "FORMIUM_TOKEN",
        "CLERK_SECRET_KEY"
      ],
      "inputs": [
        ".env*"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "bh-briet-jacket#build": {
      "outputs": [
        ".next/**"
      ]
    },
    "bh-briet-tagger#build": {
      "env": [
        "SANITY_STUDIO_PROJECTID",
        "SANITY_STUDIO_DATASET"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
